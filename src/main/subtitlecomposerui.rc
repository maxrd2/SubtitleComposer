				<Action name="recent_tr_subtitles" />
				<Separator />
				<Action name="save_tr_subtitle" />
				<Action name="save_tr_subtitle_as" />
				<Separator />
				<Action name="close_tr_subtitle" />
			</Menu>
		</Menu>
		<Menu name="edit" >
			<text>&amp;Edit</text>
			<Action name="undo" />
			<Action name="redo" />
