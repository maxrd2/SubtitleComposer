<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui name="subtitlecomposer" version="2" >
	<MenuBar>
		<Menu name="file" >
			<text>&amp;File</text>
			<Action name="new_subtitle" />
			<Action name="open_subtitle" />
			<Action name="recent_subtitles" />
