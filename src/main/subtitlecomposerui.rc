		<Menu name="texts" >
			<text>&amp;Texts</text>
			<Action name="adjust_texts" />
			<Action name="unbreak_texts" />
			<Action name="simplify_spaces" />
			<Action name="change_case" />
			<Action name="fix_punctuation" />
			<Action name="translate"/>
			<Separator />
			<Action name="spell_check" />
		</Menu>
		<Menu name="styles" >
			<text>St&amp;yles</text>
