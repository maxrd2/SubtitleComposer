		<Menu name="audiolevels" >
			<text>&amp;Audio Levels</text>
			<Action name="open_audiolevels" />
			<Action name="recent_audiolevels" />
			<Separator />
			<Action name="save_audiolevels_as" />
			<Separator />
			<Action name="close_audiolevels" />
			<Separator />
			<Action name="increase_audiolevels_v_zoom" />
			<Action name="decrease_audiolevels_v_zoom" />
			<Action name="increase_audiolevels_h_zoom" />
			<Action name="decrease_audiolevels_h_zoom" />
		</Menu>
		-->
	</MenuBar>
	<ToolBar alreadyVisited="1" noMerge="1" name="mainToolBar" >
		<text>Main Toolbar</text>
		<Action name="new_subtitle" />
		<Action name="open_subtitle" />
		<Separator name="separator_0" />
		<Action name="save_subtitle" />
		<Action name="save_subtitle_as" />
		<Separator name="separator_1" />
		<Action name="close_subtitle" />
		<Separator name="separator_2" />
		<Action name="change_subtitle_encoding" />
	</ToolBar>
	<ToolBar alreadyVisited="1" noMerge="1" name="editToolBar" >
		<text context="The toolbar name">Edit Toolbar</text>
		<Action name="undo" />
		<Action name="redo" />
		<Separator name="separator_0" />
		<Action name="toggle_selected_lines_bold" />
		<Action name="toggle_selected_lines_italic" />
		<Action name="toggle_selected_lines_underline" />
		<Action name="toggle_selected_lines_strikethrough" />
		<Separator name="separator_1" />
		<Action name="find" />
		<Action name="find_previous" />
		<Action name="find_next" />
		<Separator name="separator_2" />
		<Action name="spell_check" />
